    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-flowtype": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "inquirer": "^3.3.0",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.4",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "react-addons-perf": "^15.4.2",
