{
  "name": "bmo-economics",
  "version": "0.0.1",
  "description": "front end development for BMO Economics application",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run local",
    "start-analyze": "BUNDLE_ANALYZE=true npm run local",
    "env:local": "cross-env HOST_ENV=local NODE_ENV=development",
    "local": "npm run dumbledore & npm run env:local -- webpack-dev-server --progress --config webpack.config.dev.js",
    "env:bmo_economics_dev": "cross-env HOST_ENV=dev NODE_ENV=production",
    "env:bmo_economics_qa": "cross-env HOST_ENV=qa NODE_ENV=production",
    "env:bmo_economics_uat": "cross-env HOST_ENV=uat NODE_ENV=production",
    "env:bmo_economics": "cross-env HOST_ENV=prod NODE_ENV=production",
    "build:clean:economics": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy:economics": "cpx 'public/**/*' dist",
    "prebuild_economics": "npm run build:clean:economics && npm run build:copy:economics",
    "build:bmo_economics_dev": "npm run prebuild_economics && npm run env:bmo_economics_dev -- webpack --config webpack.config.prod.js",
    "build:bmo_economics_qa": "npm run prebuild_economics && npm run env:bmo_economics_qa -- webpack --config webpack.config.prod.js",
    "build:bmo_economics_uat": "npm run prebuild_economics && npm run env:bmo_economics_uat -- webpack --config webpack.config.prod.js",
    "build:bmo_economics": "npm run prebuild_economics && npm run env:bmo_economics -- webpack --config webpack.config.prod.js",
    "generate": "gulp generate",
    "g": "npm run generate",
    "flow": "flow",
    "dumbledore": "gulp dumbledore-watch",
    "gulp": "gulp",
    "dumbledore-oneshot": "gulp dumbledore-lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gale43/BMO-Economics-UI.git"
  },
  "author": "GALE Partners",
  "license": "ISC",
  "engines": {
    "node": ">=6.2.1",
    "npm": ">=3.9.3"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11"
  ],
  "pre-commit": [
    "lint"
  ],
  "webpackHtml": {
    "title": "BMO Economics",
    "gtmID": "GTM-M993LG6",
    "cmsFilePath": "cms-data",
    "local": {
      "devPublicPath": "https:\\//bmo-economics-osqa-ui.galepartners.com"
    },
    "dev": {
      "devPublicPath": ""
    },
    "qa": {
      "devPublicPath": ""
    },
    "uat": {
      "devPublicPath": ""
    },
    "prod": {
      "devPublicPath": ""
    }
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.23.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "moment": "^2.20.1",
    "node-sass": "^4.5.3",
    "react": "^15.5.4",
    "react-date-picker": "^6.11.2",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.2.0",
