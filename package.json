    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.17.0",
    "unchained-ui-react": "git+ssh://git@github.com/Gale43/GALE-Unchained-UI-React.git#4d55fec437daa9d1616c3d7daa9978596ab08be6",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.4.5",
